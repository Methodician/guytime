{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "paths": {
      "@app/*": ["app/*"],
      "@env/*": ["environments/*"],
      "@components/*": ["app/components/*"],
      "@services/*": ["app/services/*"],
      "@guards/*": ["app/guards/*"],
      "@models/*": ["app/models/*"]
    },
    "outDir": "./out-tsc/app",
    "types": []
  },
  "files": ["src/main.ts", "src/polyfills.ts"],
  "include": ["src/**/*.d.ts"]
}
